<h2> Url List </h2>
<ul>
  <% @urls.each do |url| %>
    <li>
      <%= link_to url.name, url.origin_url, target: "_blank" %>
      <%= link_to 'Edit', edit_compressed_url_path(url), class: 'btn btn-sm btn-primary' %>
      <%= link_to 'Delete', compressed_url_path(url), method: :delete, class: 'btn btn-sm btn-danger' %>
    </li>
  <% end %>
</ul>
<%= link_to 'Add Url', new_compressed_url_path %>
